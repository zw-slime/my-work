{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "no-console": false,
    "no-var-requires": false,
    "ordered-imports": false,
    "member-access": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "semicolon": false,
    "no-empty-interface": false,
    "no-any": false,
    "trailing-comma": false,
    "no-string-literal": false,
    "no-empty": false
  },
  "rulesDirectory": []
}